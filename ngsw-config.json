{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://localhost/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name":"tasks",
      "urls": [
        "https://localhost:3000/tasks"
      ],
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "72h"
      }
    }
  ]
}
